{
  "name": "calvin.me",
  "description": "Personal website running on Gatsby, React, and Node.js.",
  "version": "3.0.0",
  "author": "Calvin Bui",
  "repository": "git@github.com:calvinbui/calvin.me",
  "dependencies": {
    "eslint-utils": "^3.0.0",
    "gatsby": "^3.7.2",
    "gatsby-cli": "^3.7.1",
    "gatsby-plugin-catch-links": "^3.7.1",
    "gatsby-plugin-feed": "^3.7.1",
    "gatsby-plugin-image": "^1.7.1",
    "gatsby-plugin-manifest": "^3.7.1",
    "gatsby-plugin-netlify": "^3.7.1",
    "gatsby-plugin-nprogress": "^3.7.1",
    "gatsby-plugin-offline": "^4.7.1",
    "gatsby-plugin-react-helmet": "4.7.1",
    "gatsby-plugin-sass": "^4.7.1",
    "gatsby-plugin-sharp": "^3.7.1",
    "gatsby-plugin-sitemap": "4.3.1",
    "gatsby-plugin-umami": "^0.1.3",
    "gatsby-remark-autolink-headers": "^4.4.1",
    "gatsby-remark-copy-linked-files": "^4.4.1",
    "gatsby-remark-images": "5.4.1",
    "gatsby-remark-prismjs": "5.4.1",
    "gatsby-remark-responsive-iframe": "^4.4.1",
    "gatsby-remark-table-of-contents": "^1.0.0",
    "gatsby-source-filesystem": "^3.7.1",
    "gatsby-transformer-remark": "4.4.1",
    "gatsby-transformer-sharp": "^3.7.1",
    "lodash": "^4.17.13",
    "lodash.kebabcase": "^4.1.1",
    "moment": "^2.25.3",
    "node-sass": "^6.0.0",
    "prismjs": "^1.20.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.0.0",
    "react-icons": "^4.2.0",
    "sharp": "^0.28.3",
    "to": "^0.2.9",
    "update": "^0.7.4",
    "url-join": "^4.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "cli-glob": "^0.1.0",
    "eslint": "^7.29.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.24.0",
    "prettier": "^2.3.1",
    "remark-cli": "^9.0.0",
    "remark-preset-lint-recommended": "^5.0.0",
    "stylefmt": "^6.0.3",
    "stylelint": "^13.3.3",
    "stylelint-config-standard": "^22.0.0",
    "write-good": "^1.0.1"
  },
  "keywords": [
    "calvin bui",
    "development",
    "dev",
    "tutorials",
    "tech",
    "articles",
    "blog"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "develop": "gatsby develop",
    "dev": "yarn run develop",
    "serve": "gatsby serve",
    "build": "gatsby build",
    "build:pp": "gatsby build --prefix-paths",
    "build:gh": "yarn run clean && yarn run build:pp && gh-pages -d public",
    "clean": "rm -rf public && rm -rf .cache",
    "lint:js": "eslint --ext .js,.jsx .",
    "lint:md": "remark posts/*/.md",
    "lint:scss": "npx stylelint src/styles/**/*.scss",
    "write-good": "write-good $(glob 'posts/*/*.md')",
    "format:js": "prettier '**/*.{js,jsx}' --write",
    "check": "yarn upgrade-interactive --latest"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-recommended"
    ]
  }
}
