  "repository": "git@github.com:calvinbui/calvinbui.github.io",
  "dependencies": {
    "eslint-utils": "^2.0.0",
    "gatsby": "^2.20.25",
    "gatsby-cli": "^2.11.12",
    "gatsby-image": "^2.3.4",
    "gatsby-plugin-catch-links": "^2.2.3",
    "gatsby-plugin-feed": "^2.4.3",
    "gatsby-plugin-manifest": "^2.3.5",
    "gatsby-plugin-netlify": "^2.2.3",
    "gatsby-plugin-nprogress": "^2.2.3",
    "gatsby-plugin-offline": "^3.1.4",
